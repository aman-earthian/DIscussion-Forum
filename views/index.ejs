<% include ./partials/header %>

<h1>Home Page</h1>

<% forums.forEach(function(forum){ %>
    <div >
        <h2><%= forum.question%></h2>  
        <img src="<%= forum.image%>" >
        <span><%= forum.created%></span>
        <p><%= forum.answer%>....</p>  
        <a href="/forums/<%=forum._id%>"> Read More</a>
    </div>
<% }) %>

<% include ./partials/footer %>